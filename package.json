{
  "name": "cross-stitch",
  "version": "1.0.0",
  "description": "The backend server for Cross Stitch Pattern Designer",
  "engines": {
    "node": ">= 7.0.0"
  },
  "main": "index.js",
  "scripts": {
    "deploy": "script/deploy",
    "heroku-token": "script/encrypt-heroku-auth-token.js",
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint-fix": "npm run lint -- --fix",
    "prepare": "if [ -d .git ]; then npm-merge-driver install; fi",
    "prettify": "prettier --write \"**/*.{js,jsx,json,css,scss,md}\"",
    "postinstall": "touch secrets.js",
    "seed": "node script/seed.js",
    "start": "node server",
    "start-server": "nodemon server -e html,js,scss",
    "test":
      "NODE_ENV='test' mocha \"./server/**/*.spec.js\" \"./script/**/*.spec.js\" --require @babel/polyfill --require @babel/register"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": ["prettier --write", "npm run lint-fix", "git add"],
    "*.{css,scss,json,md}": ["prettier --write", "git add"]
  },
  "author": "Meredyth Houpos",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.3",
    "compression": "^1.7.3",
    "connect-session-sequelize": "^6.0.0",
    "express": "^4.16.4",
    "express-session": "^1.15.1",
    "history": "^4.9.0",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-google-oauth": "^2.0.0",
    "pg": "^7.9.0",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.7.2",
    "puppeteer": "^2.0.0",
    "sequelize": "^5.21.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-config-fullstack": "^6.0.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-react": "^7.12.4",
    "git-url-parse": "^11.1.2",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "mocha": "^6.1.4",
    "nodemon": "^1.18.3",
    "npm-merge-driver": "^2.3.5",
    "prettier": "1.11.1",
    "simple-git": "^3.5.0",
    "supertest": "^4.0.2",
    "yaml": "^1.6.0"
  }
}
